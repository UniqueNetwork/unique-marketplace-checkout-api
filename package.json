{
  "name": "unique-marketplace-api",
  "version": "1.8.201",
  "description": "Backend project for unique marketplace",
  "author": "Unique Network",
  "private": true,
  "license": "Apache 2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "coverage": "jest  --config tests/jest.config.ts --coverage --detectOpenHandles",
    "test": "jest  --config tests/jest.config.ts --detectOpenHandles",
    "test:escrow": "jest  --config tests/jest.config.ts --detectOpenHandles tests/escrow.unique.test.ts",
    "test:watch": "jest  --config tests/jest.config.ts --watch --detectOpenHandles",
    "escrow:kusama": "node dist/run.js start_escrow kusama --auction",
    "escrow:unique": "node dist/run.js start_escrow unique",
    "check:collection": "npx nestjs-command check collection",
    "check:token": "npx nestjs-command check collection",
    "check:config": "npx nestjs-command checkconfig",
    "deploy:contract": "npx nestjs-command deploy contract",
    "deploy:market": "npx nestjs-command deploymarket",
    "typeorm": "npx typeorm-ts-node-esm -d src/database/connection-options.ts",
    "migration:generate": "npx typeorm-ts-node-esm -- migration:generate -n",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d src/database/connection-options.ts",
    "migration:show": "npx typeorm-ts-node-esm migration:show -d src/database/connection-options.ts",
    "migration:revert": "npm run typeorm -- migration:revert",
    "schema:log": "npm run typeorm -- schema:log",
    "prepare": "husky install",
    "release:major": "changelog -M",
    "release:minor": "changelog -m",
    "release:patch": "changelog -p",
    "docs": "npx @compodoc/compodoc -p tsconfig.json -s",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/platform-socket.io": "^9.0.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@nestjs/websockets": "^9.0.5",
    "@polkadot/api": "8.7.1",
    "@polkadot/keyring": "9.7.2",
    "@polkadot/networks": "9.3.1",
    "@polkadot/types": "8.7.1",
    "@polkadot/util": "9.7.2",
    "@polkadot/util-crypto": "9.7.2",
    "@sentry/node": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "@sentry/types": "^7.7.0",
    "@socket.io/postgres-adapter": "^0.3.0",
    "@socket.io/postgres-emitter": "^0.1.0",
    "@types/cli-color": "^2.0.2",
    "@unique-nft/accounts": "^0.3.1",
    "@unique-nft/opal-testnet-types": "^0.5.3",
    "@unique-nft/quartz-mainnet-types": "^0.6.1",
    "@unique-nft/substrate-client": "^0.5.7",
    "@unique-nft/unique-mainnet-types": "^0.6.3",
    "checkout-sdk-node": "^2.0.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cli-color": "^2.0.1",
    "dotenv": "^16.0.1",
    "ethereum-input-data-decoder": "0.3.5",
    "ethers": "^5.5.4",
    "helmet": "^5.1.0",
    "lint-staged": "^13.0.1",
    "nestjs-command": "3.1.2",
    "pg": "8.7.1",
    "pg-connection-string": "^2.5.0",
    "pino": "^8.4.0",
    "pino-pretty": "^9.0.0",
    "prom-client": "^14.0.1",
    "prometheus-api-metrics": "^3.2.2",
    "protobufjs": "7.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "set-interval-async": "^2.0.3",
    "swagger-ui-express": "4.5.0",
    "terminal-kit": "^2.5.1",
    "typeorm": "^0.3.7",
    "typeorm-extension": "^2.1.5",
    "uuid": "8.3.2",
    "web3": "1.7.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/testing": "^9.0.5",
    "@sentry/types": "^7.7.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.0.0",
    "@types/set-interval-async": "^1.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "generate-changelog": "^1.8.0",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "prettier": "^2.7.0",
    "socket.io-client": "^4.4.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix"
  }
}
